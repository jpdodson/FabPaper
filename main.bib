	Bdsk-Url-1 = {http://dx.doi.org/10.1103/PhysRevLett.92.037901}}

@article{Friesen:2005p708,
	Abstract = {We develop an effective mass theory for substitutional donors in silicon in an inhomogeneous environment. Valley-orbit coupling is included perturbatively. We specifically consider the Stark effect in SiP. In this case, the theory becomes more accurate at high fields, while it is designed to give correct experimental binding energies at zero field. Unexpectedly, the ground state energy for the donor electron is found to increase with electric field as a consequence of spectrum narrowing of the 1s manifold. Our results are of particular importance for the Kane quantum computer.},
	Affiliation = {Univ Wisconsin, Dept Phys, Madison, WI 53706 USA},
	Author = {M Friesen},
	Doi = {10.1103/PhysRevLett.94.186403},
	Journal = {Phys. Rev. Lett.},

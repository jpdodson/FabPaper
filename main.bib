	Title = {Coherent manipulation of coupled electron spins in semiconductor quantum dots},
	Volume = {309},
	Year = {2005},
	Bdsk-Url-1 = {http://dx.doi.org/10.1126/science.1116955}}

@article{Petta:2006p472,
	Abstract = {We use gate voltage control of the exchange interaction to prepare, manipulate, and measure two-electron spin states in a GaAs double quantum dot. By placing two electrons in a single dot at low temperatures we prepare the system in a spin singlet state. The spin singlet is spatially separated by transferring an electron to an adjacent dot. The spatially separated spin singlet state dephases in similar to 10 ns due to the contact hyperfine interaction with the GaAs host nuclei. To combat the hyperfine dephasing, we develop quantum control techniques based on fast electrical control of the exchange interaction. We demonstrate coherent spin-state rotations in a singlet-triplet qubit and harness the coherent rotations to implement a singlet-triplet spin echo refocusing pulse sequence. The singlet-triplet spin echo extends the spin coherence time to 1.2 mu s. (c) 2006 Elsevier B.V. All rights reserved.},
	Affiliation = {Harvard Univ, Dept Phys, Cambridge, MA 02138 USA},
	Author = {J. R Petta and A. C Johnson and J. M Taylor and E. A Laird and A Yacoby and M. D Lukin and C. M Marcus and M. P Hanson and A. C Gossard},
	Doi = {10.1016/j.physe.2006.08.020},
	Journal = {Physica E},
	Month = {Jan},
	Pages = {251--256},
	Title = {Preparing, manipulating, and measuring quantum states on a chip},
	Volume = {35},
	Year = {2006},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.physe.2006.08.020}}

@article{Petta:2006p474,
	Abstract = {We demonstrate high-speed manipulation of a few-electron double quantum dot. In the one-electron regime, the double dot forms a charge qubit. Microwaves are used to drive transitions between the (1,0) and (0, 1) charge states of the double dot. A local quantum point contact charge detector measures the photon-induced change in occupancy of the charge states. Charge detection is used to measure T-l similar to 16 ns and also provides a lower bound estimate for T-2(*) of 400 ps for the charge qubit. In the two-electron regime we use pulsed-gate techniques to measure the singlet-triplet relaxation time for nearly-degenerate spin states. These experiments demonstrate that the hyperfine interaction leads to fast spin relaxation at low magnetic fields. Finally, we discuss how two-electron spin states can be used to form a logical spin qubit. (c) 2006 Elsevier B.V. All rights reserved.},
	Affiliation = {Harvard Univ, Dept Phys, Cambridge, MA 02138 USA},
	Author = {J. R Petta and A. C Johnson and J. M Taylor and A Yacoby and M. D Lukin and C. M Marcus and M. P Hanson and A. C Gossard},

@article{deSousa:2001p1354,
	Abstract = {We calculate the effect of an inhomogeneous magnetic field, which is invariably present in an experimental environment, on the exchange energy of a double-quantum-dot artificial molecule, projected to be used as a two-qubit quantum gate in the proposed quantum-dot quantum computer. We use two different theoretical methods to calculate the Hilbert space structure in the presence of the inhomogeneous field: the Heitler-London method, which is carried out analytically, and the molecular orbital method. which is done computational ly. Within these approximations we show that the exchange energy J changes slowly when the coupled dots are subject to a magnetic field with a wide range of inhomogeneity, suggesting that swap operations can be performed in such an environment as long as quantum error correction is applied to account for the Zeeman term. We also point out the quantum interference nature of this slow variation in exchange.},
	Affiliation = {Univ Maryland, Dept Phys, College Pk, MD 20742 USA},
	Author = {de Sousa, R. and Hu, X. D. and Das Sarma, S.},
	Journal = {Phys. Rev. A},
	Month = {Jan},
	Number = {4},
	Pages = {042307},
	Title = {Effect of an inhomogeneous external magnetic field on a quantum-dot quantum computer},
	Volume = {64},
	Year = {2001}}

@article{deSousa:2003p1358,
	Abstract = {We theoretically consider coherence times for spins in two quantum computer architectures, where the qubit is the spin of an electron bound to a P donor impurity in Si or within a GaAs quantum dot. We show that low-temperature decoherence is dominated by spin-spin interactions, through spectral diffusion and dipolar flip-flop mechanisms. These contributions lead to 1-100-mus calculated spin coherence times for a wide range of parameters, much higher than former estimates based on T-2* measurements.},
	Affiliation = {Univ Maryland, Dept Phys, Condensed Matter Theory Ctr, College Pk, MD 20742 USA},
	Author = {de Sousa, R. and Das Sarma, S.},
	Doi = {10.1103/PhysRevB.67.033301},

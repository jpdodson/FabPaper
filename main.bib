@article{Nowack:2007p1430,
	Abstract = {Manipulation of single spins is essential for spin-based quantum information processing. Electrical control instead of magnetic control is particularly appealing for this purpose, because electric fields are easy to generate locally on-chip. We experimentally realized coherent control of a single-electron spin in a quantum dot using an oscillating electric field generated by a local gate. The electric field induced coherent transitions (Rabi oscillations) between spin-up and spin-down with 90 rotations as fast as similar to 55 nanoseconds. Our analysis indicated that the electrically induced spin transitions were mediated by the spin-orbit interaction. Taken together with the recently demonstrated coherent exchange of two neighboring spins, our results establish the feasibility of fully electrical manipulation of spin qubits.},
	Affiliation = {Delft Univ Technol, Kavli Inst Nanosci, NL-2600 GA Delft, Netherlands},

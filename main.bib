@article{Kang:2000p70,
	Abstract = {We report observations of collective gap excitations of the fractional quantum Hall (FQH) states at filling factors nu = p/(2p + 1) (p = integer), for 1/3 less than or equal to nu less than or equal to 2/3, by inelastic light scattering. The collective gap energies at nu = 1/3, 2/5, and 3/7 show a drastic decrease as the value nu = 1/2 is approached. These energies and the one at nu = 3/5 display the linear scaling with (e(2)/epsilon l(0))/\ 2p + 1 \ that is characteristic of composite fermions in Chern-Simons gauge fields. In a narrow range of nu centered at 1/2, where the FQH gaps collapse, we observe a new excitation mode which exists only at temperatures below 150 mK. PACS numbers: 73.40.Hm, 73.20.Mf, 78.30.-j.},
	Affiliation = {Columbia Univ, Dept Phys, New York, NY 10027 USA},
	Author = {M Kang and A Pinczuk and BS Dennis and MA Eriksson and LN Pfeiffer and KW West},
	Journal = {Phys. Rev. Lett.},
	Month = {Jan},
	Pages = {546--549},
	Title = {Inelastic light scattering by gap excitations of fractional quantum Hall states at 1/3 <= v <= 2/3},
	Volume = {84},
	Year = {2000}}

@article{Kang:2000p77,
	Abstract = {We report an inelastic light scattering study of long wavelength collective gap excitations of fractional quantum Hall (FQH) states at v = p/(2p + 1) for 1/3 less than or equal to v less than or equal to 2/3. The v-dependence of the gap energy suggests a collapse of the collective excitation gap near v = 1/2. In a range of filling factors close to v = 1/2, where the FQH gap is believed to collapse, we observe a collective gap near v = 1/2. In a range of filling factors close to v = 1/2, where the FQH gap is believed to collapse, we observe a collective excitation mode that exists only at temperatures below 150 mK. (C) 2000 Elsevier Science B.V. All rights reserved.},

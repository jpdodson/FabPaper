	Month = {Jan},
	Pages = {842--849},
	Title = {Multilayer PDMS microfluidic chamber for controlling brain slice microenvironment},
	Volume = {7},
	Year = {2007},
	Bdsk-Url-1 = {http://dx.doi.org/10.1039/b704754a}}

